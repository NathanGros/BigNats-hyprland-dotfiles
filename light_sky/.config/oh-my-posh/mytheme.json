{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "style": "folder"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "foreground": "#eff1f5",
          "background": "#2c2f49",
          "type": "path",
          "style": "diamond"
        },
        {
          "options": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " \uf09b {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#2c2f49> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#df8e1d{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#40a02b{{ end }}",
            "{{ if gt .Ahead 0 }}#8839ef{{ end }}",
            "{{ if gt .Behind 0 }}#8839ef{{ end }}"
          ]
        },
        {
          "options": {
            "style": "roundrock",
            "threshold": 0
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " \uf017  {{ .FormattedMs }}\u2800",
          "foreground": "#f5f5f5",
          "background": "#2c2f49",
          "type": "executiontime",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "fetch_package_manager": true,
            "npm_icon": " <#2c2f49>\ue5fa</> ",
            "yarn_icon": " <#40a02b>\ue6a7</>"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "node",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "python",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "java",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "dotnet",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "go",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue7a8 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "rust",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "dart",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "angular",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>Î± {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "aurelia",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "nx",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b><#d20f39>\ue624</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "julia",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\uf104<#df8e1d>\uf0e7</>\uf105 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "azfunc",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "<b>\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "aws",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "<b>\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}</b>",
          "foreground": "#2c2f49",
          "background": "#40a02b",
          "type": "kubectl",
          "style": "diamond"
        },
        {
          "options": {
            "linux": "\ue712",
            "macos": "\ue711",
            "windows": "\ue70f"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#eff1f5",
          "background": "#2c2f49",
          "type": "os",
          "style": "diamond"
        },
        {
          "options": {
            "time_format": "15:04"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#eff1f5",
          "background": "#2c2f49",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "always_enabled": true
          },
          "template": " \ue285 ",
          "foreground": "#2c2f49",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#d20f39{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4
}
